<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

 <path>fuel.ac</path>

  <animation>
    <object-name>fuelcock</object-name>
  </animation>
  
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>fuelcock</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>consumables/fuel/tank-selector</property>
        <!--<value type="bool">true</value>-->
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>fuelcock</object-name>
    <property>consumables/fuel/tank-selector</property>
    <factor>90</factor>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
